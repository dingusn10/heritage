---
import Layout from "../layouts/Layout.astro"

const response = await fetch(`https://api.planningcenteronline.com/people/v2/people?where[membership]=elder?client_id=${import.meta.env.PUBLIC_PCO_ID}&client_secret=${import.meta.env.PUBLIC_PCO_KEY}`);
const elders = await response.json();

---

<div>
    <h1>Elders</h1>
    {JSON.stringify(elders.results)}
</div>